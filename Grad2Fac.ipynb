    "from sklearn.model_selection import permutation_test_score\n",
    "\n",
    "k = 4\n",
    "kernel = 1.0 * Matern(length_scale=0.5, length_scale_bounds=(0.1, 1), nu=2.5) + WhiteKernel(noise_level_bounds=[0.00001,1],noise_level=0.05)\n",
    "kf = KFold(n_splits=k, random_state=None)\n",
    "\n",
    "\n",
    "\n",
    "gpr = GaussianProcessRegressor(kernel=kernel, random_state=None,normalize_y=True,alpha=0.1)\n",
    "X=Grad_TaskCentres\n",
    "scores = {}\n",
    "\n",
    "\n",
    "standardscaler=StandardScaler()\n",
    "\n",
    "y = FAC_TaskCentres\n",
    "\n",
    "#@ignore_warnings(category=ConvergenceWarning)\n",
    "def ___f():\n",
    "    score_gradfac, perm_scores_gradfac, pvalue_gradfac = permutation_test_score(gpr, X, y, scoring=\"neg_mean_absolute_error\", cv=kf, n_permutations=100,n_jobs=-1)\n",
    "\n",
    "    scores.update({1:[score_gradfac,pvalue_gradfac]})\n",
    "___f()\n"
   ]
